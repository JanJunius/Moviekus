@inherits RatingBarComponent

    <div class="pagination">
        @for (var i = 1; i <= 5; i++)
        {
            string source = i <= Rating ? "/images/star.png" : "/images/star_empty.png";
            <img src="@source" style="width:24px;height:24px;" alt="Star" class="image-display"> 
        }
    </div>
